    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch GetIP",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/GetIP/bin/Debug/netcoreapp3.1/GetIP.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
       

        {
            "name": "Launch ComputerTest",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/ComputersTest/bin/Debug/netcoreapp3.0/ComputersTest.dll",
            "args": [],
            "cwd": "${workspaceFolder}/ComputersTest",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}